{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "OPENAI_API_KEY": "@openai_api_key"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "OPENAI_API_KEY": "@openai_api_key"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "OPENAI_API_KEY": "@openai_api_key"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
